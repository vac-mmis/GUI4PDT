{
    "name": "user-interface",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "npm run type-check && npm run build-only",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/ doc/",
        "typedoc": "typedoc --tsconfig tsconfig.doc.json && vue-docgen && prettier --write doc/",
        "doc:dev": "npm run typedoc && vitepress dev doc",
        "doc:build": "npm run typedoc && vitepress build doc",
        "doc:preview": "vitepress preview doc"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "marked": "^7.0.4",
        "marked-katex-extension": "^3.0.3",
        "mathjs": "^11.9.1",
        "pinia": "^2.1.6",
        "plotly.js-dist-min": "^2.25.2",
        "sass": "^1.66.0",
        "three": "^0.155.0",
        "vue": "^3.3.4",
        "vue-dompurify-html": "^4.1.4",
        "vue-router": "^4.2.4",
        "vue3-moveable": "^0.26.1",
        "vuetify": "^3.3.13"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@tsconfig/node18": "^18.2.0",
        "@types/jsdom": "^21.1.1",
        "@types/marked": "^5.0.1",
        "@types/node": "^20.5.0",
        "@types/plotly.js-dist-min": "^2.3.1",
        "@types/three": "^0.155.0",
        "@vitejs/plugin-vue": "^4.3.1",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/test-utils": "^2.4.1",
        "@vue/tsconfig": "^0.4.0",
        "concurrently": "^8.2.0",
        "eslint": "^8.47.0",
        "eslint-plugin-vue": "^9.17.0",
        "jsdom": "^22.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.2",
        "typedoc": "^0.24.8",
        "typedoc-plugin-markdown": "^4.0.0-next.18",
        "typedoc-vitepress-theme": "^1.0.0-next.0",
        "typescript": "~5.1.6",
        "vite": "^4.4.9",
        "vitepress": "^1.0.0-rc.4",
        "vitest": "^0.34.2",
        "vue-component-meta": "^1.8.8",
        "vue-docgen-cli": "^4.67.0",
        "vue-tsc": "^1.8.8"
    }
}
